<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= homeData %></p>
<% for(var i=0; i<userBlog.length; i++){ %>
    <p><h1><%= userBlog[i].title %></h1></p>
    <% var data=userBlog[i].content %>
    <% if(data.length>100){ %>
        <% data=data.substring(0,100)+"..."; %>
        <p><%= data %><a href="/<%= userBlog[i]._id %>">read more</a></p>
    <% } else { %>
        <p><%= data %></p>
    <% } %>
<% } %>
<%- include("partials/footer") -%>